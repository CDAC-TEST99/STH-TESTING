<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN"
	"http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">

<struts-config>
	<!-- ========== Form Bean Definitions =================================== -->
	<form-beans>
		<form-bean name="DoctorDeskFB" type="new_opd.transaction.controller.fb.DoctorDeskFB" />
		<form-bean name="IPDDoctorDeskFB" type="new_ipd.transaction.fb.IPDDoctorDeskFB" />
		<form-bean name="SearchemrActionFB" type="new_opd.transaction.controller.fb.SearchemrActionFB" />
		<form-bean name="OPDTemplateMstFb" type="new_opd.transaction.controller.fb.OPDTemplateMstFb" />
		<form-bean name="IpdDoctorDeskFB" type="new_opd.transaction.controller.fb.IpdDoctorDeskFB" />
		
		<form-bean name="mcPathLabSampleDrawnDetailFB" type="new_opd.transaction.controller.fb.MCPathLabSampleDrawnDetailFB" />		
		<form-bean name="mcPathLabSampleHandoverDetailFB" type="new_opd.transaction.controller.fb.MCPathLabSampleHandoverDetailFB"/>
		<form-bean name="mcPathLabReportDetailBean" type="new_opd.transaction.controller.fb.MCPathLabReportDetailFB"/>
	    <form-bean name="OPDQMSFB" type="new_opd.transaction.controller.fb.OPDQMSFB" />
	    <form-bean name="OPDReferralFB" type="new_opd.transaction.controller.fb.OPDReferralFB" />
	</form-beans>
	
	
	<action-mappings>
	
		
		
		<action path="/transaction/DoctorDeskAction"
				type="new_opd.transaction.controller.action.DoctorDeskAction"
				name="DoctorDeskFB"
				validate="false"
				scope="request"
				parameter="hmode">
	
				<forward name="INITIAL" path="/initial_drDesk.jsp" />
				<forward name="NEW" path="/opdDrDesk.jsp" />
				<forward name="NEW1" path="/printPrescription.jsp" />
				<forward name="TEMPLATESAVE" path="/TemplateSave.jsp" />
				<forward name="MOBILEPRINTPRESC" path="/mobileprintPrescription.jsp" />
				<forward name="FILEUPLOAD" path="/fileUpload.jsp" />
				<forward name="PRESCRIPTIONBOOKMARKING" path="/prescriptionprofileList.jsp" />
				<forward name="PRINTPRESCRIPTIONBOOKMARKING" path="/printPrescription_profile.jsp" />
				<forward name="REFER_LISTING" path="/internalReferralList.jsp" />
				<forward name="REFER_HISTORY_LISTING" path="/externalReferralHistoryList.jsp" />
				<forward name="PATIENT_REVIEW" path="/patientReview.jsp" />
				<forward name="OPEN_POOL_PATIENT" path="/opdPoolPatientList.jsp" />
								
			</action>
			
			
			
			
			<action path="/transaction/IPDDoctorDeskAction"
				type="new_ipd.transaction.action.IPDDoctorDeskAction"
				name="IPDDoctorDeskFB"
				validate="false"
				scope="request"
				parameter="hmode">
<!--			<forward name="NEW" path="/transaction/medicalCertificate.jsp" />-->
			<forward name="INITIAL" path="/initial_drDesk.jsp" />
			<forward name="NEW" path="/../new_ipd/ipdDrDesk.jsp" />
			<forward name="NEW1" path="/../new_ipd/printPrescription.jsp" />
			<forward name="TEMPLATESAVE" path="/../new_ipd/TemplateSave.jsp" />
			<forward name="MOBILEPRINTPRESC" path="/mobileprintPrescription.jsp" />
			<forward name="FILEUPLOAD" path="/../new_ipd/fileUpload.jsp" />
			<forward name="PRESCRIPTIONBOOKMARKING" path="/../new_ipd/prescriptionprofileList.jsp" />
			<forward name="PRINTPRESCRIPTIONBOOKMARKING" path="/printPrescription_profile.jsp" />
			
			
			<!-- <forward name="POPUP" path="/transaction/mcDiagnosisPopup.jsp" />	
			<forward name="CANCEL" path="../startup/initPage.jsp" redirect="true" />
			<forward name="GETDTL" path="/transaction/medicalCertificateIssue.jsp"/>
			<forward name="POPUPCERT" path="/transaction/medicalCertPopup.jsp" />	 -->
			
			
		</action>
		
	
	  <action path="/transaction/IpdDoctorDeskAction"
				type="new_opd.transaction.controller.action.IpdDoctorDeskAction"
				name="IpdDoctorDeskFB"
				validate="false"
				scope="request"
				parameter="hmode">
<!--			<forward name="NEW" path="/transaction/medicalCertificate.jsp" />-->
			<forward name="INITIAL" path="/initial_drDesk.jsp" />
			<forward name="NEW" path="/ipdDrDesk.jsp" />
			<forward name="NEW1" path="/ipdprintPrescription.jsp" />
			<forward name="TEMPLATESAVE" path="/TemplateSave.jsp" />
			<forward name="MOBILEPRINTPRESC" path="/mobileprintPrescription.jsp" />
			
			<forward name="FILEUPLOAD" path="/fileUpload.jsp" />
			<forward name="PRESCRIPTIONBOOKMARKING" path="/prescriptionprofileList.jsp" />
			<forward name="PRINTPRESCRIPTIONBOOKMARKING" path="/printPrescription_profile.jsp" />
			<!-- <forward name="POPUP" path="/transaction/mcDiagnosisPopup.jsp" />	
			<forward name="CANCEL" path="../startup/initPage.jsp" redirect="true" />
			<forward name="GETDTL" path="/transaction/medicalCertificateIssue.jsp"/>
			<forward name="POPUPCERT" path="/transaction/medicalCertPopup.jsp" />	 -->
			
			
		</action>
		
		
		
		<action path="/transaction/SearchemrAction"
				type="new_opd.transaction.controller.action.SearchemrAction"
				name="SearchemrActionFB"
				validate="false"
				scope="request"
				parameter="hmode">
			<forward name="INITIAL" path="/Searchemr.jsp" />
			</action>
		
		
			<action path="/transaction/OPDTemplateMstAction"
				type="new_opd.transaction.controller.action.OPDTemplateMstAction"
				name="OPDTemplateMstFb"
				validate="false"
				scope="request"
				parameter="hmode">
			<forward name="INITIAL" path="/OPDTemplateMstListing.jsp" />
			<forward name="ADD" path="/OPDTemplateMst.jsp" />
			<forward name="MODIFY" path="/OPDTemplateModify.jsp" />
			<forward name="PRINTPRESC" path="/PreViewOPDTemplateMst.jsp" />
			<forward name="PRINTPRESCVIEW" path="/PreViewOPDTemplateMstView.jsp" />
			</action>
			
		<action path="/transaction/MCPathLabSampleDrawnDetailCNT"
			type="new_opd.transaction.controller.action.MCPathLabSampleDrawnDetailCNT"
			name="mcPathLabSampleDrawnDetailFB" scope="request" validate="false"
			parameter="hmode">
			
			<forward name="issue" path="/mc_pathlab_sampleDrawnDetail_new.jsp" />
			<forward name="issueGo" path="/mc_pathlab_sampleDrawnDetail_go_new.jsp" />		 
		</action>		
		
		<action path="/transaction/MCPathLabSampleHandoverDetailTransMcNewCNT"
			type="new_opd.transaction.controller.action.MCPathLabSampleHandoverDetailTransMcNewCNT"
			name="mcPathLabSampleHandoverDetailFB" scope="request" validate="false"
			parameter="hmode">
			
			<forward name="issue" path="/MCPathLabSampleHandoverDetail.jsp" />
			<forward name="webServicesSent" path="/mcPathLabSampleHandoverDetail_Webservices.jsp" />		 
		</action> 
		
		<action path="/transaction/MCPathLabReportDetailCNT"
			type="new_opd.transaction.controller.action.MCPathLabReportDetailCNT"
			name="mcPathLabReportDetailBean" scope="request" validate="false" parameter="hmode">
			<forward name="issue" path="/mc_pathlab_report_detail.jsp" />
			<forward name="issueGo" path="/mc_pathlab_report_detail_go.jsp" />
		</action>
		
		
		<action path="/transaction/OPDQMSAction"
				type="new_opd.transaction.controller.action.OPDQMSAction"
				name="OPDQMSFB"
				validate="false"
				scope="request"
				parameter="hmode">
				<forward name="INITIAL" path="/qmsInitPage.jsp" />				
		</action>
		<action path="/transaction/OPDDisplayBoard"
				type="new_opd.transaction.controller.action.OPDDisplayBoard"
				name="OPDQMSFB"
				validate="false"
				scope="request"
				parameter="hmode">				
				<forward name="QMS" path="/qmsDisplayBoardPage.jsp" />
		</action>
		<action path="/transaction/OPDQueueDisplayForPatient"
				type="new_opd.transaction.controller.action.OPDQueueDisplayForPatient"
				name="OPDQMSFB"
				validate="false"
				scope="request"
				parameter="hmode">				
				<forward name="QMS" path="/qmsPatientDisplayBoardPage.jsp" />
		</action>		
				<action path="/transaction/OPDReferralAction"
				type="new_opd.transaction.controller.action.OPDReferralAction"
				name="OPDReferralFB"
				validate="false"
				scope="request"
				parameter="hmode">				
				<forward name="OREFERRAL" path="/opdReferralAction.jsp" />
		</action>
		
	</action-mappings>
		
</struts-config>	

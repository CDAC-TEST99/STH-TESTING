<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN"
	"http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">

<struts-config>
	<!-- ========== Form Bean Definitions =================================== -->
	<form-beans>
		<form-bean name="DoctorDeskFB" type="new_opd.transaction.controller.fb.DoctorDeskFB" /> 
	</form-beans>
	<action-mappings>
	
			
			<action path="/pb"
				type="new_opd.transaction.controller.action.PrintPatientPrescriptionAction"
				name="DoctorDeskFB"
				validate="false"
				scope="request"
				parameter="hmode">
				<forward name="NEW1" path="/printPrescriptionForPatient.jsp" />
			
			</action>
			
			
					
	</action-mappings>
		
</struts-config>	

}

.about .content h3 {
    font-weight: 600;
    font-size: 26px;
    color: var(--es-black);
    margin: 15px 0;
    text-align: center;
}

.about .content ul {
    list-style: none;
    padding: 0;
}

.about .content ul li {
    display: flex;

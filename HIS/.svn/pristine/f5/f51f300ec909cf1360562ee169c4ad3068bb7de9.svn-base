<script>
var tr='';
	$(document).ready(function() {

		 

		
		
		$('[name="ByTenderOrRC"]').change(function(){
			
			var trobj=$(this).closest('tr');			
			if(this.value=="RC"){
				$(trobj).find('.tender').addClass('hideData');
				$(trobj).find('#rc').removeClass('hideData');
				$(trobj).find('#Add').addClass('hideData');
				$(trobj).find('#Modify').addClassClass('hideData');
				$(trobj).find('#tenderno').addClass('hideData');
				
			}
			else{
				if($(trobj).find('.tender').hasClass('hideData')){
					$(trobj).find('.tender').removeClass('hideData');
				}

				$(trobj).find('#tenderno').removeClass('hideData');
				$(trobj).find('#rc').addClass('hideData');

				
			}
		});


$('[name="tenderno"]').change(function(){
			
			var trobj=$(this).closest('tr');			
			if(this.value=="-1"){
				//$(trobj).find('.tender').addClass('hideData');
				$(trobj).find('#Add').removeClass('hideData');
				//$(trobj).find('#Modify').addClass('hideData');
				$(trobj).find('#techBidDate').val('');

				$(trobj).find('#priceBidDate').val('');
				//$(trobj).find('#tenderno').addClass('hideData');
				
			}
			else if(this.value=="0"){
				$(trobj).find('#Add').addClass('hideData');
				//$(trobj).find('#Modify').addClass('hideData');
				$(trobj).find('#techBidDate').val('');

				$(trobj).find('#priceBidDate').val('');

				
			}
			else{
					

				
				$(trobj).find('#Add').addClass('hideData');
				//$(trobj).find('#Modify').removeClass('hideData');

				$(trobj).find('#techBidDate').val('10-Feb-2020');

				$(trobj).find('#priceBidDate').val('17-Feb-2020');
					

				}
		});


$('.btnAdd').click(function() {
	tr=$(this).closest('tr');
	$('#divAddTender').modal('show');
});

$('.btnModify').click(function() {
	tr=$(this).closest('tr');
	$('#divModifyTender').modal('show');
	
	$("#techDateMod").val($(tr).find('#techBidDate').val());
	$("#priceDateMod").val($(tr).find('#priceBidDate').val());
	$("#tenderRCRefNo").val($(tr).find('#tenderno').val());
});

$("#save").click(function() {

	$(tr).find('#techBidDate').val($("#techDate").val());

	$(tr).find('#priceBidDate').val($("#priceDate").val());

	$('#divAddTender').modal('toggle');
	
});

$("#update").click(function() {

	$(tr).find('#techBidDate').val($("#techDateMod").val());

	$(tr).find('#priceBidDate').val($("#priceDateMod").val());

	$('#divModifyTender').modal('toggle');
	
});

		
		
		$('#btnSearchIndent').click(function(){
			$('#openIndentList').modal('show');	
		});
		
		$('.addInst').click(function() {
			$("#openIndentList").modal('hide');
			$('#indentRefEntry').addClass('hideData')
			$('#indentRefView').removeClass('hideData')
			$('#divIndent').removeClass('hideData')
			 var sourceTRObj = $(this).closest('tr');
			var prevIndentRefNo = $(sourceTRObj).find('[data-title="Indent Reference No."]').html();
			var prevIndentDate = $(sourceTRObj).find('[data-title="Date"]').html();
			var prevIndentType = $(sourceTRObj).find('[data-title="Indent Type"]').html();
			
			$('#prevIndentType').html(prevIndentType);
			$('#prevIndentRefNo').html(prevIndentRefNo);
			$('#prevIndentDate').html(prevIndentDate); 
		});
		$('#btnGo').click(function(){
			$('#indentRefEntry').addClass('hideData');
			$('#indentRefView').removeClass('hideData');
			$('#divIndent').removeClass('hideData'); 
			$('#prevIndentType').html("G.O.");
			$('#prevIndentRefNo').html($('#indentRefNo').val());
			$('#prevIndentDate').html("02-Jan-2020");
		});
		 
		
	});
</script>


<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>CGHSCardMgmt</display-name>
  <session-config>
    <cookie-config>
      <name>TID</name>
      <http-only>true</http-only>
    </cookie-config>
  </session-config>
  <servlet>
    <servlet-name>FormFlowXACTION</servlet-name>
    <servlet-class>formFlowX.action.FormFlowXACTION</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FormFlowXACTION</servlet-name>
    <url-pattern>/FormFlowXACTION</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>BhavishyaAction</servlet-name>
    <servlet-class>thirdpartyservices.bhavishya.service.BhavishyaAction</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BhavishyaAction</servlet-name>
    <url-pattern>/BhavishyaAction</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>BharatkoshAction</servlet-name>
    <servlet-class>thirdpartyservices.bharatkosh.service.BharatkoshAction</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BharatkoshAction</servlet-name>
    <url-pattern>/BharatkoshAction</url-pattern>
  </servlet-mapping>
  
    
    <listener>
      <listener-class>thirdpartyservices.bhavishya.scheduler.SchedulerContextListener</listener-class>
   </listener>
   <listener>
      <listener-class>thirdpartyservices.bharatkosh.service.BKPayTrackSchedulerContextListener</listener-class>
   </listener>
  
  <listener>
    <listener-class>org.jboss.resteasy.plugins.server.servlet.ResteasyBootstrap</listener-class>
  </listener>
  <listener>
    <listener-class>hissso.client.listener.HISSSOClientContextListener</listener-class>
  </listener>
  <listener>
    <listener-class>hissso.client.listener.HISSSOClientSessionListener</listener-class>
  </listener>
  <filter>
    <filter-name>HISSSOClientRequestFilter</filter-name>
    <filter-class>hissso.client.filter.HISSSOClientRequestFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>HISSSOClientRequestFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping> 
   <filter>
    <filter-name>CorsFilter</filter-name>
    <filter-class>formFlowX.service.CorsFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>CorsFilter</filter-name>
    <url-pattern>/services/restful/*</url-pattern>
  </filter-mapping>
  
  <filter>
    <filter-name>JwtAuthenticationFilter</filter-name>
    <filter-class>formFlowX.service.JwtAuthenticationFilter</filter-class>
  </filter>
  <filter-mapping>
  <filter-name>JwtAuthenticationFilter</filter-name>
  <url-pattern>/services/restful/parliament/*</url-pattern>
</filter-mapping>

<filter-mapping>
  <filter-name>JwtAuthenticationFilter</filter-name>
  <url-pattern>/services/restful/nha/tms/v1.0/benificary/airindiareferral</url-pattern>
</filter-mapping>
  
 <filter-mapping>
  <filter-name>JwtAuthenticationFilter</filter-name>
  <url-pattern>/services/restful/nha/tms/v1.0/benificary/token</url-pattern>
</filter-mapping> 
<filter-mapping>
  <filter-name>JwtAuthenticationFilter</filter-name>
  <url-pattern>/services/restful/ogd/v1/details/*</url-pattern>
</filter-mapping> 
  <listener>
    <listener-class>org.jboss.resteasy.plugins.server.servlet.ResteasyBootstrap</listener-class>
  </listener>
  <context-param>
    <param-name>resteasy.scan</param-name>
    <param-value>true</param-value>
  </context-param>
  <context-param>
    <param-name>resteasy.servlet.mapping.prefix</param-name>
    <param-value>/services/restful</param-value>
  </context-param>
  <servlet>
    <servlet-name>Resteasy</servlet-name>
    <servlet-class>org.jboss.resteasy.plugins.server.servlet.HttpServletDispatcher</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Resteasy</servlet-name>
    <url-pattern>/services/restful/*</url-pattern>
  </servlet-mapping>
  <error-page>
    <error-code>403</error-code>
    <location>/application/jsp/app_error_under.jsp</location>
  </error-page>
  <error-page>
    <error-code>404</error-code>
    <location>/application/jsp/app_error_under.jsp</location>
  </error-page>
  <error-page>
    <error-code>500</error-code>
    <location>/application/jsp/app_error_under.jsp</location>
  </error-page>
  <error-page>
    <error-code>503</error-code>
    <location>/application/jsp/app_error_under.jsp</location>
  </error-page>
  <error-page>
    <exception-type>javax.servlet.ServletException</exception-type>
    <location>/application/jsp/app_error_under.jsp</location>
  </error-page>
  <error-page>
    <exception-type>java.io.IOException</exception-type>
    <location>/application/jsp/app_error_under.jsp</location>
  </error-page>
  <error-page>
    <exception-type>java.lang.IllegalStateException</exception-type>
    <location>/application/jsp/app_error_under.jsp</location>
  </error-page>
</web-app>
jcs.region.ModuleWiseServiceConfigurationCache.cacheattributes.UseMemoryShrinker=true
jcs.region.ModuleWiseServiceConfigurationCache.cacheattributes.MaxMemoryIdleTimeSeconds=72000
jcs.region.ModuleWiseServiceConfigurationCache.cacheattributes.ShrinkerIntervalSeconds=60
jcs.region.ModuleWiseServiceConfigurationCache.cacheattributes.MaxSpoolPerRun=2000
jcs.region.ModuleWiseServiceConfigurationCache.elementattributes=org.apache.jcs.engine.ElementAttributes
jcs.region.ModuleWiseServiceConfigurationCache.elementattributes.IsEternal=false


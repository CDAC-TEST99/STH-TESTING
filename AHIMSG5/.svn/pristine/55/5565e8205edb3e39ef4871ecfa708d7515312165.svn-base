@media (max-width: 991px) {
	
	.handlingdivwidth .col{ flex:none !important}
	
	.navbar {     position: absolute !important;
    top: 58px;
    right: 6px;}
    header .main-menu {
    background-color: transparent;
}
	
	.pdfcon {width:86% !important; }
	
	.navbar-toggler .close-icon {
		display: none;
	}

	.navbar-toggler .close-icon:focus-visible {
		border: 0;
		outline: 0;
	}

	.navbar-toggler[aria-expanded="true"] .navbar-toggler-icon {
		display: none;
	}

	.navbar-toggler[aria-expanded="true"] .close-icon {
		display: inline-block;
		position: fixed;
		top: 5px;
		right: 10px;
		transition: 0.3s ease-in-out;
		z-index: 9999999;
	}

	.navbar-nav .nav-item .dropdown-menu {
		display: none;
	}

	.navbar-collapse {
		position: fixed; display: block !important;
		top: 0;
		right: 0;
		height: 100%;
		width: 250px;
		background-color: rgba(0, 0, 0, 0.9);
		box-shadow: -5px 0 10px rgba(0, 0, 0, 0.1);
		transform: translateX(100%);
		transition: transform 0.3s ease-in-out;
		z-index: 999999;
	}

	.navbar-collapse.show {
		transform: translateX(0);
	}

	.navbar-nav .nav-item>a {
		color: rgba(255, 255, 255, 0.9);
		font-weight: 500;
		font-size: 0.845rem;
		padding: 10px 14px;

	}
	.navbar-expand-lg .navbar-nav .nav-link
 {
    padding: 10px 18px;
}

	.navbar-nav .nav-link.active,
	.navbar-nav .show>.nav-link {
		color: rgba(255, 255, 255, 0.9);
	}

	.navbar-nav .nav-item>a span {
		margin-top: 0;
		margin-left: 10px;
	}

	.dropdown-menu .dropdown-item {
		white-space: normal;
	}

	.navbar-nav .nav-item .dropdown-menu li a {
		padding: 5px 0px;
		font-size: 13px;
	}

	.navbar-nav .nav-item .dropdown-menu {
		padding: 10px 10px;
	}


	#slider .cghsbanner .carousel-caption p {
		font-size: 13px;
	}

	.footer-links li+li {
		padding: 0 0px;
	}

	.footer-links li:after {
		content: none;
	}

	.all-logins span {
		display: none;
	}

	.loginbuttton span {
		display: none;
	}

	.loginbuttton {
		padding:2px 2px; text-align: center;
	}

}


